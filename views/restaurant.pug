extends layout

block content
  h1= title
  - const restaurant = ['Room_type','Duration','Cost']
  - let results;
  script.
    function buy(x){
      alert('Purchased Item:- ' + x)
   } 
  div(class='container-fluid')
    div(class='row')
      each results, index in restaurant
        div(class='col-sm-2' onclick='buy('+index+')') #{results} 
        

  
